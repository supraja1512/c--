#include<iostream>
using namespace std;
int add(int a,int b)
{
    return a+b;
}
float add(float a,float b)
{
    return a+b;
}
double add(double a,double b)
{
    return a+b;   
}
int main()
{
    int a,b;
    cin>>a>>b;
    cout<<add(a,b);
    float c,d;
    cin>>c>>d;
    cout<<add(c,d);
    double e,f;
    cin>>e>>f;
    cout<<add(e,f);
}
    
