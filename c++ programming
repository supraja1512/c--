#include<iostream>
using namespace std;
int main()
{
   int n;
   cin>>n;
   for(int i=0;i<4;i++)
   {
       for(int j=0;j<10;j++)
       {
           if((i==1)&&(j!=0)&&(i==1)&& (j!=9) ||(i==2)&&(j!=0)&&(i==2)&&(j!=9))
           { 
               
              
              cout<<" ";
           
           }
           
           else
           {
               cout<<n;
           }
       }
      cout<<endl;
   }
}
