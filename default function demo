#include<iostream>
using namespace std;
int sum(int a,int b,int c,int d);
int sum1(int a,int b,int c,int d=0);
int sum2(int a,int b,int c=0,int d=0);
int main()
{
    int a,b,c,d;
    cout<<sum(3,4,5,7)<<endl;
    cout<<sum1(3,4,5)<<endl;
    cout<<sum2(3,4);
}
int sum(int a,int b,int c,int d)
{
    return a+b+c+d;
}
int sum1(int a,int b,int c,int d)
{
    return a+b+c+d;
}
int sum2(int a,int b,int c,int d)
{
    return a+b+c+d;
}
